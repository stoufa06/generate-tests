{
    "name": "stoufa06/generate-tests",
    "description": "A script to generate tests for projects",
    "type": "project",
    "require" : {
        "php" : "^8.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.4"
    },
    "autoload": {
        "psr-4": {
            "Stoufa06\\GenerateTests\\": "src/"
        }
    },
    "bin": [
        "bin/generate-tests"
    ],
    "config": {
        "optimize-autoloader": true
    },
    "scripts": {
        "test": "phpunit tests"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Mustapha A",
            "homepage": "https://github.com/stoufa06/"
        }
    ]
}
